<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TableLayout
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center|center_horizontal"
        android:padding="30dp"
        android:baselineAligned="false"
        android:touchscreenBlocksFocus="false"
        android:textAlignment="center">


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">

            <ImageView android:id="@+id/imageView11"
                android:layout_column="0"
                android:src="@drawable/dinner"
                android:layout_width="99dp"
                android:layout_height="99dp"
                android:paddingTop="10dp"
                android:paddingBottom="3dp" />

            <ImageView android:id="@+id/imageView12"
                android:layout_column="1"
                android:src="@drawable/breakfast"
                android:layout_width="99dp"
                android:layout_height="99dp"
                android:paddingTop="10dp"
                android:paddingBottom="3dp" />

            <ImageView android:id="@+id/imageView13"
                android:layout_column="2"
                android:src="@drawable/party"
                android:layout_width="99dp"
                android:layout_height="99dp"
                android:paddingTop="10dp"
                android:paddingBottom="3dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal">

            <TextView
                android:text="Dinner on \n12 Sep"
                android:id="@+id/textView23"
                android:layout_column="0"
                android:layout_width="99dp"
                android:layout_gravity="center"
                android:textColor="#000000"
                android:layout_height="wrap_content"
                android:paddingBottom="10dp"
                android:gravity="center" />

            <TextView
                android:text="Breakfast on \n13 Sep"
                android:id="@+id/textView24"
                android:layout_column="1"
                android:layout_width="99dp"
                android:layout_gravity="center"
                android:textColor="#000000"
                android:layout_height="wrap_content"
                android:paddingBottom="10dp"
                android:gravity="center" />

            <TextView
                android:text="Pool Party on \n18 Sep"
                android:id="@+id/textView25"
                android:layout_column="2"
                android:layout_width="99dp"
                android:layout_gravity="center"
                android:textColor="#000000"
                android:layout_height="wrap_content"
                android:paddingBottom="10dp"
                android:gravity="center" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/timorlodgelogooriginal"
                android:layout_column="1" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tourdetown"
                android:src="@drawable/tour"
                android:layout_column="1" />
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:text="Tour de Town"
                android:id="@+id/textView26"
                android:layout_width="99dp"
                android:layout_gravity="center"
                android:textColor="#000000"
                android:layout_height="wrap_content"
                android:paddingBottom="10dp"
                android:gravity="center"
                android:layout_column="1"/>
        </TableRow>
    </TableLayout>
    </RelativeLayout>
